# Global set
set automatic_html_opening False

# Import model
import model sm-no_b_mass

# Define multiparticles
define p- = p b b~
define j- = j b b~

# Specify process(es) to run
generate p p > z, z > l+ l-
add process p p > z j, z > l+ l-
add process p p > z j j, z > l+ l-

# Output processes to MadEvent directory
output MYPATH -nojpeg
launch MYPATH

# ME + PS / LO
shower=Pythia8

#run_card (FIXED except xqcut)
set run_card ickkw 1
set run_card xqcut 10.0
set run_card ptj 0.01
set run_card ptl 0
set run_card draa 0
set run_card draj 0.1
set run_card dral 0.1
set run_card asrwgtflavor 5
set run_card maxjetflavor 5 #if you use b quark with massless i.e. sm-no_b_mass
